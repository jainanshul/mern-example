{
  "name": "turbo",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "^9.0.0"
  },
  "scripts": {
    "start": "concurrently \"babel-node server/server.js\" \"cd client && npm run build\"",
